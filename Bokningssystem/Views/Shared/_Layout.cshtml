<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Bokningssystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Bokningssystem.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="templates/assets/css/main.css" asp-append-version="true" />
    <noscript><link rel="stylesheet" href="templates/assets/css/noscript.css" /></noscript>
</head>

    <body class="is-preload">

        <!-- Header -->
        <header id="header">
            <a class="title">Kvarnnibble musteri</a>
            <nav>
                <ul>
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Hem</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Booking">Bokning</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="Faq">Frågor & svar</a></li>
                </ul>
            </nav>
        </header>

        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>

        <!-- Footer -->
        <footer id="footer" class="wrapper alt">
            <div class="inner">
                <ul class="menu">
                    <li>&copy; Untitled. All rights reserved.</li>
                    <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
                </ul>
            </div>
        </footer>

        <!-- Scripts -->
@*         <script src="templates/assets/js/jquery.min.js"></script>
        <script src="templates/assets/js/jquery.scrollex.min.js"></script>
        <script src="templates/assets/js/jquery.scrolly.min.js"></script>
        <script src="templates/assets/js/browser.min.js"></script>
        <script src="templates/assets/js/breakpoints.min.js"></script>
        <script src="templates/assets/js/util.js"></script>
        <script src="templates/assets/js/main.js"></script> *@

        @await RenderSectionAsync("Scripts", required: false)
    </body>

</html>
